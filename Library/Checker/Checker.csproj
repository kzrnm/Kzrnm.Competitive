<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFrameworks>net7.0;netcoreapp3.1</TargetFrameworks>
    <LangVersion>11</LangVersion>
    <RootNamespace>Kzrnm.Competitive</RootNamespace>
    <Configurations>Debug;Release;Library</Configurations>
    <StartupObject>Program</StartupObject>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AtCoderAnalyzer" Version="3.2.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="CompetitiveVerifierProblem" Version="*" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Competitive.Library\Competitive.Library.csproj" />
  </ItemGroup>

</Project>
